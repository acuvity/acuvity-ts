{
  "name": "@acuvity/acuvity",
  "version": "0.5.1",
  "author": "Acuvity, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acuvity/acuvity-ts.git"
  },
  "bugs": {
    "url": "https://github.com/acuvity/acuvity-ts/issues"
  },
  "description": "The official Acuvity TypeScript SDK",
  "homepage": "https://acuvity.ai/",
  "keywords": [
    "security",
    "genai",
    "ai",
    "prompt_detection",
    "jailbreak",
    "typescript",
    "library",
    "nodejs"
  ],
  "type": "module",
  "tshy": {
    "sourceDialects": [
      "@acuvity/acuvity/source"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./types": "./src/types/index.ts",
      "./models/errors": "./src/models/errors/index.ts",
      "./models/components": "./src/models/components/index.ts",
      "./*.js": "./src/*.ts",
      "./*": "./src/*.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 src",
    "build": "tshy",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "zod": ">= 3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/deno": "^2.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.0.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tshy": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.22.0",
    "zod": "^3.23.4"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "yaml": "^2.7.0"
  }
}
